# Environment Variables Template
# Copy this file to .env.local and fill in your actual API keys
# NEVER commit .env.local to git!

# Formspree Configuration (Fallback)
VITE_FORMSPREE_ENDPOINT=https://formspree.io/f/your_form_id

# EmailJS Configuration
VITE_EMAILJS_SERVICE_ID=your_service_id
VITE_EMAILJS_TEMPLATE_ID=your_template_id
VITE_EMAILJS_USER_ID=your_user_id

# AI Service Configuration (Primary - AI21 Studio)
VITE_AI21_API_KEY=your_ai21_api_key_here
AI21_API_KEY=your_ai21_api_key_here

# OpenAI API (Secondary fallback)
VITE_OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# OpenRouter API (Final fallback)
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Vercel Edge Config (Optional)
EDGE_CONFIG=your_edge_config_url_here